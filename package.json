{
  "name": "discord-bot-aoo",
  "version": "0.1.0",
  "author": "Alexis Besson me@alexis-besson.dev <https://alexis-besson.dev/>",
  "license": "GPL-3.0-or-later",
  "description": "Discord bot specially created for Albion Online game, it allows player to add/list/remove objectives (nodes, cores and vortexes) with all the necessary informations like the type of objective, it's rarity, the map and the time remaining before objective unlock",
  "keywords": [
    "Discord",
    "Bot",
    "Albion Online",
    "Objectives",
    "Slash commands"
  ],
  "main": "dist/src/main.js",
  "type": "module",
  "scripts": {
    "sync-db": "node dist/scripts/sync-db.js",
    "build": "tsc && tsc-alias",
    "dev": "pnpm run build && node --env-file=.env.dev dist/src/main.js",
    "start": "node dist/src/main.js"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "dependencies": {
    "@logtail/pino": "0.5.7",
    "@sapphire/framework": "5.4.0",
    "@sequelize/core": "7.0.0-alpha.47",
    "@sequelize/postgres": "7.0.0-alpha.47",
    "dayjs": "1.11.19",
    "discord.js": "14.25.1",
    "node-cron": "4.2.1",
    "pino": "10.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@sapphire/ts-config": "5.0.3",
    "@types/node": "22.14.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.1",
    "globals": "16.5.0",
    "pino-pretty": "13.1.3",
    "tsc-alias": "1.8.16",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0"
  }
}
