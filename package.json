{
  "name": "discord-bot-aoo",
  "version": "0.1.0",
  "author": "Alexis Besson me@alexis-besson.dev <https://alexis-besson.dev/>",
  "license": "GPL-3.0-or-later",
  "description": "Discord bot specially created for Albion Online game, it allows player to add/list/remove objectives (nodes, cores and vortexes) with all the necessary informations like the type of objective, it's rarity, the map and the time remaining before objective unlock",
  "keywords": [
    "Discord",
    "Bot",
    "Albion Online",
    "Objectives",
    "Slash commands"
  ],
  "main": "dist/src/main.js",
  "type": "module",
  "scripts": {
    "sync-db": "node dist/scripts/sync-db.js",
    "build": "tsc && tsc-alias",
    "dev": "pnpm run build && node --env-file=.env.dev dist/src/main.js",
    "start": "node dist/src/main.js"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a",
  "dependencies": {
    "@logtail/pino": "0.5.7",
    "@sapphire/framework": "5.5.0",
    "@sequelize/core": "7.0.0-alpha.47",
    "@sequelize/postgres": "7.0.0-alpha.47",
    "dayjs": "1.11.19",
    "discord.js": "14.25.1",
    "node-cron": "4.2.1",
    "pino": "10.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@sapphire/ts-config": "5.0.3",
    "@types/node": "24.10.11",
    "dotenv": "17.2.4",
    "eslint": "9.39.2",
    "globals": "17.3.0",
    "pino-pretty": "13.1.3",
    "tsc-alias": "1.8.16",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0"
  }
}
